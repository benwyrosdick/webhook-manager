{
  "name": "webhook-manager",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:backend": "dotenv -e .env.local -- nodemon server.js",
    "dev:frontend": "vite",
    "dev": "concurrently \"yarn dev:frontend\" \"yarn dev:backend\"",
    "start": "node server.js",
    "build": "npx vite build && cp dist/index.html public/ && cp -r dist/assets public/ && cp dist/*.svg public/",
    "build:start": "yarn build && yarn start",
    "dev:ngrok": "ngrok http 3001",
    "prisma:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
    "prisma:generate": "dotenv -e .env.local -- npx prisma generate",
    "prisma:studio": "dotenv -e .env.local -- npx prisma studio",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@prisma/client": "6.12.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "axios": "^1.7.9",
    "body-parser": "^1.20.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "lucide-react": "^0.525.0",
    "prisma": "6.12.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "shadcn-ui": "^0.9.5",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^6.0.1",
    "vitest": "^1.0.0"
  }
}
